       url("/fonts/gilroy-bold.woff") format("woff");
  font-display: swap;
}

@font-face {
  font-family: "gilroy";
  font-weight: 900;
  src: url("/fonts/gilroy-black.woff2") format("woff2"),
       url("/fonts/gilroy-black.woff") format("woff");
  font-display: swap;
}
