  font-weight: 500;
  src: url("/fonts/gilroy-medium.woff2") format("woff2"),
       url("/fonts/gilroy-medium.woff") format("woff");
  font-display: swap;
}

@font-face {
  font-family: "gilroy";
  font-weight: 700;
