.prev,
.next {
	color: $accentColor;
}

@media (min-width: 720px) {
  .page-nav {
		max-width: none;
    position: fixed;
    bottom: 0;
    right: 0;
		left: 340px;
		padding: 0 30px 30px 30px;
    background-color: greyLight;
  }
}

@media (min-width: 960px) {
  .page-nav .inner {
    margin: 0 auto;
		min-height: auto;
		padding: 0;
		border: 0;
  }
}
